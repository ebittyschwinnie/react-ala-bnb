  },
  "scripts": {
    "dev": "webpack-dev-server --hot",
    "build": "webpack -p",
    "test": "jest --watchAll",
    "coverage": "jest --coverage",
    "lint": "eslint \"{src,test}/**/*.{js,jsx}\"",
    "prettier": "prettier --write \"{src,test}/**/*.{js,jsx,css}\""
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/{components,containers}/**/*.{js,jsx}"
    ],
    "verbose": true
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "proseWrap": "always",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
